<h1>Users</h1>
<table class="table table-condensed table-hover">
  <thead>
    <tr>
      <td><strong>Username</strong></td>
    </tr>
  </thead>
  <tbody>
    <% if @users.empty? %>
    <tr>No users to display!</tr>
    <% end %>
    <% @users.each do |user| %>
    <tr>
      <td><a href="/users/<%= user.id %>"><%= user.full_name %></a></td>
    </tr>
    <% end %>
  </tbody>
</table>